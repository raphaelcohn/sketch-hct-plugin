{
	"name": "sketch-hct-plugin",
	"main": "./src/commands/generate.js",
	"packageManger": "npm@10.8.2",
	"type": "commonjs",
	"exports": [
		"./src/commands/generate.js",
		"./src/commands/Sub Menu/example.js"
	],
	"imports": {},
	"version": "0.0.0",
	"license": "AGPL-3.0-or-later",
	"private": true,
	"os": [
		"darwin",
		"linux"
	],
	"cpu": [
		"arm64",
		"x64"
	],
	"dependencies": {
		"@material/material-color-utilities": "0.3.0"
	},
	"devDependencies": {
		"@sketch-hq/sketch-file-format-ts": "6.5.0",
		"@skpm/builder": "0.9.5",
		"esbuild": "0.24.0",
		"typescript": "5.6.2"
	},
	"engines": {
		"node": "20.17.0",
		"npm": "10.8.2",
		"sketch": ">=3.0"
	},
	"devEngines": {
		"runtime": {
			"name": "node",
			"version": "20.17.0",
