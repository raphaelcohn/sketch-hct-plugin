{
	"name": "sketch-hct-plugin",
	"main": "./src/commands/generate.js",
	"packageManger": "npm@10.8.2",
	"type": "commonjs",
	"exports":
	[
		"./src/commands/generate.js",
		"./src/commands/Sub Menu/example.js"
	],
	"imports":
